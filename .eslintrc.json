{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "parser": "babel-eslint",
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "linebreak-style": [
            "error",
            "windows"
        ],
        "react/prop-types": [0],
        "react/display-name": [0],
        "react/jsx-filename-extension":"off",
        "import/no-unresolved":"off",
        "no-console": "off",
        "consistent-return": "off"
    }
}
